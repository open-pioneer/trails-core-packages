{
    "name": "starter",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "clean": "rimraf dist",
        "dev": "vite",
        "watch-types": "tsc -w --noEmit",
        "build": "tsc && vite build",
        "build-plugin": "cd vite-plugin && pnpm run build",
        "preview": "vite preview",
        "lint": "eslint ./src ./vite-plugin",
        "prettier": "prettier ./src ./docs ./vite-plugin -w --ignore-path .eslintignore",
        "test": "vitest"
    },
    "devDependencies": {
        "@types/node": "^16.18.11",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.9",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "@vitejs/plugin-react": "^3.0.0",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-react": "^7.32.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "jsdom": "^21.1.0",
        "prettier": "^2.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.9.3",
        "vite": "^4.0.4",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-pioneer": "workspace:*",
        "vitest": "^0.28.1"
    },
    "engines": {
        "node": ">= 16",
        "pnpm": "^7"
    }
}
