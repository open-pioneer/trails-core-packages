{
    "name": "starter",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "clean": "rimraf dist",
        "dev": "pnpm run build-plugin && vite",
        "check-types": "tsc --noEmit",
        "watch-types": "pnpm check-types -w",
        "build": "pnpm run build-plugin && tsc && vite build",
        "build-plugin": "cd build-tools/vite-plugin && pnpm run build-js",
        "test-plugin": "cd build-tools/vite-plugin && pnpm run build-js && pnpm run test",
        "preview": "vite preview",
        "lint": "eslint ./src ./build-tools",
        "prettier-check": "pnpm prettier:common -c",
        "prettier": "pnpm prettier:common -w",
        "prettier:common": "prettier ./src ./docs ./build-tools --ignore-path .eslintignore",
        "test": "pnpm run build-plugin && vitest",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@open-pioneer/vite-plugin-pioneer": "workspace:*",
        "@types/node": "^16.18.11",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.9",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "@vitejs/plugin-react": "^3.0.0",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-react": "^7.32.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.0",
        "jsdom": "^21.1.0",
        "prettier": "^2.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.9.3",
        "vite": "^4.0.4",
        "vite-plugin-eslint": "^1.8.1",
        "vitest": "^0.28.1"
    },
    "engines": {
        "node": ">= 16",
        "pnpm": "^7"
    }
}
