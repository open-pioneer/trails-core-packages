{
    "name": "starter",
    "private": true,
    "type": "module",
    "license": "Apache-2.0",
    "scripts": {
        "clean": "rimraf dist",
        "dev": "vite",
        "check-types": "tsc --noEmit",
        "watch-types": "pnpm check-types -w",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint ./src --cache",
        "prettier-check": "pnpm prettier:common -c",
        "prettier": "pnpm prettier:common -w",
        "prettier:common": "prettier ./src ./docs --ignore-path .eslintignore --cache",
        "test": "vitest",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@open-pioneer/build-support": "^0.2.0",
        "@open-pioneer/runtime": "workspace:*",
        "@open-pioneer/vite-plugin-pioneer": "^0.3.3",
        "@testing-library/dom": "^8.20.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/node": "^16.18.12",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "@vitejs/plugin-react": "^3.1.0",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-header": "^3.1.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.3",
        "jsdom": "^21.1.0",
        "prettier": "^2.8.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^4.1.2",
        "rollup-plugin-visualizer": "^5.9.0",
        "typescript": "^4.9.5",
        "vite": "^4.1.4",
        "vite-plugin-eslint": "^1.8.1",
        "vitest": "^0.28.5"
    },
    "engines": {
        "node": ">= 16",
        "pnpm": "^7"
    }
}
