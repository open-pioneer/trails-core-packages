{
    "$schema": "https://typedoc.org/schema.json",
    "includeVersion": true,
    "exclude": ["**/test-data/**", "**.test.*", "**/node_modules/**", "**/samples/**"],
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeInternal": true,
    "skipErrorChecking": true,
    "validation": {
        "notExported": false,
        "invalidLink": true,
        "notDocumented": true
    }
}
